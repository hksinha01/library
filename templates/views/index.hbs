<!DOCTYPE html>
<html lang="en">


<!-- doctors23:12-->

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0">
    <link rel="shortcut icon" type="image/x-icon" href="img/library.jpg">
    <title>Noble Library</title>
    <link rel="stylesheet" type="text/css" href="css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="css/select2.min.css">
    <link rel="stylesheet" type="text/css" href="css/bootstrap-datetimepicker.min.css">
    <link rel="stylesheet" type="text/css" href="css/style.css">
</head>

<body>
    <script>

    </script>
    

    <div class="main-wrapper">
        {{>header}}
        {{>sidebar}}
        <div class="page-wrapper">
            <div class="content">
                <div class="row">
                    <div class="col-sm-4 col-3">
                        <h4 class="page-title">Books</h4>
                    </div>

                    <div class="col-sm-8 col-9 text-right m-b-20">
                        <a href="add-books" class="btn btn-primary btn-rounded float-right"><i class="fa fa-plus"></i>
                            Add Books</a>
                    </div>
                </div>
                <div class="dash-widget-info text-right">
                    <h4>{{count}}</h4>
                    <span class="widget-title1">Total Books<i class="fa fa-check" aria-hidden="true"></i></span>
                </div>
                <div class="row doctor-grid">
                    {{#each userData}}
                    
                    <div class="col-md-4 col-sm-4  col-lg-3">
                        <div class="profile-widget">
                            <div class="doctor-img">
                                <a class="avatar" href="./bookInfo?bookNumber=" +{{this.bookNumber}}><img alt=""
                                        src="img/books.png"></a>
                            </div>
                            <div class="dropdown profile-action">
                                <a href="#" class="action-icon dropdown-toggle" data-toggle="dropdown"
                                    aria-expanded="false"><i class="fa fa-ellipsis-v"></i></a>
                                <div class="dropdown-menu dropdown-menu-right">
                                    <a class="dropdown-item" href="./edit-books"><i class="fa fa-pencil m-r-5"></i>
                                        Edit</a>
                                    <a class="dropdown-item" href="./deleteBooks"><i class="fa fa-trash-o m-r-5"></i>
                                        Delete</a>
                                </div>
                            </div>
                            <h4 class="doctor-name text-ellipsis"><a href="bookInfo">{{this.title}}</a></h4>
                            <div class="doc-prof">{{this.category}}</div>
                            <div class="doc-prof">{{this.bookNumber}}</div>
                        </div>
                    </div>
                    {{/each}}


                </div>
                <div class="row">
                    <div class="col-sm-12">
                        <div class="see-all">
                            <a class="see-all-btn" href="javascript:void(0);">Load More</a>
                        </div>
                    </div>
                </div>
            </div>

        </div>

    </div>
    <div class="sidebar-overlay" data-reff=""></div>
    <script src="js/jquery-3.2.1.min.js"></script>
    <script src="js/popper.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="js/jquery.slimscroll.js"></script>
    <script src="js/select2.min.js"></script>
    <script src="js/moment.min.js"></script>
    <script src="js/bootstrap-datetimepicker.min.js"></script>
    <script src="js/app.js"></script>
</body>


<!-- doctors23:17-->

</html>